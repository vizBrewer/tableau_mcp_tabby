# remote mcp Config
TABLEAU_MCP_HTTP_URL=http://mcpstreamableurl:3927/tableau-mcp

# Model Providers (set MODEL_PROVIDER to "openai" or "aws")
MODEL_PROVIDER=openai
MODEL_USED=gpt-5
MODEL_TEMPERATURE=0

# OpenAI Configuration (required if MODEL_PROVIDER=openai)
OPENAI_API_KEY='your-openai-api-key'

# AWS Bedrock Configuration (required if MODEL_PROVIDER=aws)
# These are standard AWS credentials - boto3 will use them automatically
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1
# AWS_SESSION_TOKEN=your-session-token  # Optional, only if using temporary credentials

# Langfuse (optional - set USE_LANGFUSE=true to enable)
USE_LANGFUSE=false
LANGFUSE_PUBLIC_KEY=langfuse-public-key
LANGFUSE_SECRET_KEY=langfuse-secret-key
LANGFUSE_HOST=https://us.cloud.langfuse.com

